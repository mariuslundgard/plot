{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "9.4.0"
        }
      }
    ]
  ],
  "plugins": [
    "transform-class-properties",
    "transform-flow-strip-types",
    "transform-object-rest-spread",
    ["transform-react-jsx", {"pragma": "h"}],
    [
      "css-modules-transform",
      {
        "generateScopedName": "node-hipster-starter-[local]",
        "extensions": [".css"]
      }
    ]
  ]
}
